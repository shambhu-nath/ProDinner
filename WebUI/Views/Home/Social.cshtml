@{
    var url = "https://prodinner.aspnetawesome.com";
    var title = "ProDinner ASP.net MVC Awesome Demo";
}
<div class="example">
    <div class="awe-il" style="margin-top: -12px;">
        <button type="button" class="reddit" onclick="openw('https://www.reddit.com/submit?url=@url')">reddit</button>
    </div>
    <a href="https://twitter.com/share" class="twitter-share-button" data-url="@url"
       data-text="@title" data-count="none" data-hashtags="aspnetawesome">Tweet</a>
    <script src="//platform.linkedin.com/in.js" type="text/javascript"></script>
    <div style="position: relative; width: 59px; height: 20px; display: inline-block;">
        <div style="position: absolute;">
            <script type="IN/Share" data-url="@url">
            </script>
        </div>
    </div>
    <div class="fb-share-button"
         data-href="@url"
         data-layout="button">
    </div>
    <div class="fb-like"
         data-href="https://www.facebook.com/aspnetawesome"
         data-layout="standard"
         data-action="like"
         data-show-faces="false">
    </div>
</div>
<style>
    .fb-share-button, .fb-like {
        vertical-align: top;
        margin-top: -1px;
    }

    .reddit {
        padding: 2px 10px;
        border: 1px solid #5f99cf;
        border-radius: 2px;
        background: #cee3f8;
        cursor: pointer;
        color: #369;
        font-weight: bold;
        font-family: normal x-small verdana, arial, helvetica, sans-serif;
    }
</style>
<div id="fb-root"></div>
<script>!function (d, s, id) { var js, fjs = d.getElementsByTagName(s)[0]; if (!d.getElementById(id)) { js = d.createElement(s); js.id = id; js.src = "//platform.twitter.com/widgets.js"; fjs.parentNode.insertBefore(js, fjs); } }(document, "script", "twitter-wjs");</script>
<script>
    (function (d, s, id) {
        var js, fjs = d.getElementsByTagName(s)[0];
        if (d.getElementById(id)) return;
        js = d.createElement(s); js.id = id;
        js.src = "https://connect.facebook.net/en_US/sdk.js#xfbml=1&version=v3.0";
        fjs.parentNode.insertBefore(js, fjs);
    }(document, 'script', 'facebook-jssdk'));

    function openw(url) {
        window.open(url);
    }
</script>